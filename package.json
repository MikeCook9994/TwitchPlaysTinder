  },
  "bugs": {
    "url": "https://github.com/MikeCook9994/TwitchPlaysTinder/issues"
  },
  "homepage": "https://github.com/MikeCook9994/TwitchPlaysTinder#readme",
  "scripts": {
    "clean": "rm -rf ./dist",
    "copy-secrets": "cp ./server/app/secrets.json ./dist/server/app/", 
    "ng-build": "cd ./ux & ng build --base-href ./ & cd ..",
    "electron-build": "tsc ./main.ts --outDir ./dist & tsc & npm run copy-secrets",
    "build": "npm run clean & npm run electron-build & npm run ng-build",
    "start": "cd ./dist & electron ./main.js & cd ..",
    "install-ux": "cd ./ux & npm i & cd ..",
    "install-all": "npm i & npm run install-ux"
  },
  "dependencies": {
    "@types/electron": "^1.6.10",
    "@types/express": "^4.11.1",
    "@types/puppeteer": "^1.2.1",
    "axios": "^0.18.0",
    "electron": "^1.8.3",
    "express": "^4.16.3",
    "puppeteer": "^1.2.0"
  },
  "devDependencies": {
    "typescript": "~2.5.3"
  }
}
